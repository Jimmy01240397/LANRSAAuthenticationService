[Unit]
Description=Start LANAllowWeb for %I
After=network.target
OnFailure=lanallowweb-failure@%i.service

[Service]
WorkingDirectory=/etc/lanloginserver
ExecStart=/etc/lanloginserver/lanloginserver.sh -i %i
ExecStop=/etc/lanloginserver/stoplanloginserver.sh -i %i


[Install]
WantedBy=multi-user.target
